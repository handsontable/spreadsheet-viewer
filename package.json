{
  "name": "spreadsheet-viewer",
  "version": "1.0.1",
  "main": "src/sv/index.tsx",
  "repository": "git@github.com:handsontable/spreadsheet-viewer.git",
  "author": "Handsoncode <hello@handsontable.com>",
  "license": "UNLICENSED",
  "scripts": {
    "prepare:submodules": "git submodule update --init && yarn prepare:submodules:handsontable",
    "prepare:submodules:handsontable": "cd submodules/handsontable && cross-env NODE_ENV=production npm ci",
    "prepare": "yarn prepare:submodules",
    "start": "cross-env NODE_ENV=development yarn webpack-dev-server --host 0.0.0.0",
    "build": "yarn build:client-library && yarn build:sv",
    "build:sv": "webpack",
    "build:client-library": "cd src/client-library && tsc",
    "serve:sv": "node serve-sv.js",
    "test": "yarn test:unit  && yarn test:snapshot && yarn lint",
    "test:unit": "cross-env TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} mocha -r ts-node/register -r tsconfig-paths/register tests/unit/*.test.ts tests/unit/**/*.test.ts",
    "test:electron": "cypress run",
    "test:chrome": "cypress run --browser chrome",
    "test:gui": "cypress open",
    "test:snapshot": "cd docker && docker-compose -f test-nonlinux.yml up --exit-code-from cypress_sv",
    "test:snapshot:linux": "cd docker && docker-compose -f test-linux.yml up --exit-code-from cypress_sv && cd .. && sudo chown -R $USER cypress/*",
    "perf": "node ./node_modules/puppeteer/install.js && ts-node --project tsconfig.perf.json ./perf/performanceMeasurements.ts",
    "cov:unit": "cross-env TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} nyc -r lcov -r=text -r=text-summary  -r=json -e .ts -x \"*.test.ts\" mocha -r ts-node/register -r tsconfig-paths/register tests/unit/*.test.ts tests/unit/**/*.test.ts && nyc report && cp coverage/coverage-final.json coverage/coverage.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "clean-build": "rm -rf node_modules dist && yarn && yarn build",
    "report": "echo '# HTML file sizes' && ls -Slh dist/sv/*.html && echo '# JS bundle sizes' && ls -Slh dist/sv/*.js && echo '# CSS bundle sizes' && ls -Slh dist/sv/*.css && echo '# Total size of CSS and JS bundles' && du -ch dist/sv/*.{css,js} | tail -n 1",
    "help:report": "echo Outputs the file sizes of all JS and CSS bundles, and the total size of these bundles.",
    "testing-bot": "ts-node --dir testing-bot --project tsconfig.json index.ts",
    "testing-bot:regenerate": "yarn testing-bot --scenario=all --execution-mode=regenerate"
  },
  "alias": {
    "handsontable": "/../../submodules/handsontable/src"
  },
  "dependencies": {},
  "devDependencies": {
    "@antv/data-set": "^0.11.7",
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@handsontable/js-xlsx": "100.0.13",
    "@handsontable/react": "^3.1.3",
    "@material-ui/core": "^4.11.0",
    "@types/chai": "^4.2.13",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/material-ui": "^0.21.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/pixelmatch": "^5.2.2",
    "@types/pngjs": "^3.4.2",
    "@types/puppeteer": "^3.0.2",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/selenium-webdriver": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "abortcontroller-polyfill": "^1.5.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.1",
    "bizcharts": "^3.5.9",
    "chai": "^4.2.0",
    "connect": "^3.7.0",
    "core-js": "2.6.11",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cypress": "^5.4.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-image-snapshot": "^3.1.1",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "extract-loader": "^5.1.0",
    "fast-csv": "^4.3.2",
    "file-loader": "^6.1.1",
    "fontfaceobserver": "^2.1.0",
    "is-reachable": "^5.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.0.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "p-series": "^2.1.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer": "^5.3.1",
    "purify-ts": "0.15.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "serve-static": "^1.14.1",
    "testingbot-tunnel-launcher": "^1.1.7",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^3.4.1",
    "worker-loader": "^3.0.4"
  }
}
